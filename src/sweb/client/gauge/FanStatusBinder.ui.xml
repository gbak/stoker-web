<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>

		<g:HorizontalPanel width="100%" verticalAlignment="ALIGN_MIDDLE">
		   <g:HTML styleName="sweb-fanLabel"  >Fan:</g:HTML>
		   <g:Button styleName="sweb-fanDisplayButton" text="OFF" ui:field="fanStatusButton" width="40px" height="23px" enabled="false"/>
		   <g:Label styleName="sweb-fanElapsedTime"  ui:field="time" text="00:00:00">0</g:Label>
		</g:HorizontalPanel>

	</g:HTMLPanel>
</ui:UiBinder> 